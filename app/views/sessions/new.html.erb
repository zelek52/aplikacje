<h1>Logowanie</h1>
<%= form_for(:session, url: login_path) do |f| %>
<%= f.label :login %>
<%= f.text_field :login, class: 'form-control' %>
<%= f.label :password %>
<%= f.password_field :password, class: 'form-control' %>
<%= f.submit "Zaloguj" %>
<% end %>
